{
  "manifest_version": 3,
  "name": "Audio to Text Extension",
  "version": "1.0",
  "permissions": [
    "tts",
    "audioCapture",
    "audio",
    "tabCapture",
    "scripting",
    "activeTab",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "<all_urls>"
  ]
}